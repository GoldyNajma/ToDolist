    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Starter"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/GoldyTheme">
        <activity android:name=".modules.register.RegisterActivity" />
        <activity android:name=".modules.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.tasks.TasksActivity" />
        <activity android:name=".modules.addtask.AddTaskActivity" />
        <activity android:name=".modules.edittask.EditTaskActivity" />
        <activity android:name=".MainActivity">
<!--            <intent-filter>-->
<!--                <action android:name="android.intent.action.MAIN" />-->

<!--                <category android:name="android.intent.category.LAUNCHER" />-->
<!--            </intent-filter>-->
        </activity>
    </application>

</manifest>